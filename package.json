{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "pnpm run i18n-extract && nuxt dev",
    "generate": "pnpm run i18n-extract && nuxt generate && pnpm build-storybook",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "ssg": "pnpm run generate && pnpm run serve",
    "serve": "pnpm dlx serve .output/public",
    "check-updates": "ncu -i --format group",
    "test": "vitest -w",
    "i18n-extract": "vue-i18n-extract report --vueFiles './**/*.?(js|vue)' --languageFiles './locales/*.?(json|yml|yaml|js)' --noEmptyTranslation '*' --add true --remove true",
    "storybook": "storybook dev --port 6006",
    "build-storybook": "storybook build -o ./.output/public/_storybook/"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.22",
    "@heroicons/vue": "^2.1.5",
    "@vueuse/core": "^11.0.0",
    "animate.css": "^4.1.1",
    "pinia": "^2.2.2",
    "sitemap": "^8.0.0",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@nuxt/content": "^2.13.2",
    "@nuxt/image": "^1.7.0",
    "@nuxtjs/storybook": "^8.2.0",
    "@nuxt/test-utils": "^3.14.1",
    "@nuxtjs/color-mode": "^3.4.4",
    "@nuxtjs/i18n": "^8.4.0",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "@pinia/nuxt": "^0.5.3",
    "@tailwindcss/typography": "^0.5.14",
    "@types/node": "^18.17.5",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^14.12.3",
    "@storybook-vue/nuxt": "^8.2.0",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/builder-vite": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@storybook/vue3": "^8.2.9",
    "nuxt": "^3.13.0",
    "storybook": "^8.2.9",
    "storybook-addon-root-attributes": "^2.1.3",
    "playwright-core": "^1.46.1",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "sass": "^1.77.8",
    "vitest": "^2.0.5",
    "vue": "latest",
    "vue-i18n-extract": "^2.0.7"
  }
}