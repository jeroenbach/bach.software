    "@nuxt/image": "^1.7.0",
    "@nuxtjs/storybook": "^8.2.0",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "@tailwindcss/typography": "^0.5.14",
    "@storybook-vue/nuxt": "^8.2.0",
    "@storybook/addon-essentials": "^8.2.9",
