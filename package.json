{
  "name": "bach.software",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0 <19.0.0",
    "pnpm": ">=9.8.0 <10.0.0"
  },
  "version": "0.1.0",
  "description": "Bach Software - A personal blog and portfolio website",
  "author": "Jeroen Bach",
  "scripts": {
    "check-updates": "ncu -i --format group",
    "dev-setup": "node development-setup.js",
    "i18n-extract": "vue-i18n-extract report --vueFiles './**/*.?(js|vue)' --languageFiles './src/app/locales/*.?(json|yml|yaml|js)' --noEmptyTranslation '*' --add true --remove true",
    "build": "nuxt build",
    "generate": "pnpm run i18n-extract && nuxt generate --dotenv .env.local && pnpm storybook:build",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "dev": "pnpm run i18n-extract && nuxt dev --dotenv .env.local",
    "dev:api": "cd ./src/api/Bach.Software.API && dotnet watch start",
    "dev:static-web-app": "cp staticwebapp.config.json .output/public && swa start .output/public --api-location src/api/Bach.Software.API",
    "ssg": "nuxt generate && pnpm run serve",
    "serve": "pnpm dlx serve ./.output/public/",
    "test": "TZ=Europe/Amsterdam vitest -w",
    "ci:test": "TZ=Europe/Amsterdam vitest run",
    "ci:type-check": "nuxi typecheck",
    "storybook": "storybook dev --config-dir ./src/app/.storybook --port 6006",
    "storybook:build": "echo 'Temporary turned off the storybook build'",
    "storybook:build:disabled": "storybook build -c ./src/app/.storybook -o ./.output/public/_storybook/"
  },
  "dependencies": {
    "@headlessui/vue": "1.7.23",
    "@heroicons/vue": "2.1.5",
    "@primevue/themes": "4.1.0",
    "@vueuse/core": "11.1.0",
    "animate.css": "4.1.1",
    "dayjs": "1.11.13",
    "inquirer": "^9.2.15",
    "luxon": "3.5.0",
    "nuxt-content-twoslash": "0.1.1",
    "pinia": "2.2.4",
    "primevue": "4.1.0",
    "remark-reading-time": "2.0.1",
    "sitemap": "8.0.0",
    "slugify": "1.6.6",
    "tailwindcss-primeui": "0.3.4",
    "typescript": "5.6.3"
  },
  "devDependencies": {
    "@nuxt/content": "2.13.2",
    "@nuxt/image": "1.8.1",
    "@nuxt/test-utils": "3.14.3",
    "@nuxtjs/color-mode": "3.5.1",
    "@nuxtjs/i18n": "8.5.5",
    "@nuxtjs/partytown": "1.5.0",
    "@nuxtjs/plausible": "1.2.0",
    "@nuxtjs/storybook": "8.3.0",
    "@nuxtjs/tailwindcss": "6.12.1",
    "@pinia/nuxt": "0.5.5",
    "@primevue/nuxt-module": "4.1.0",
    "@storybook-vue/nuxt": "8.3.0",
    "@storybook/addon-essentials": "8.3.0 ",
    "@storybook/addon-interactions": "8.3.0 ",
    "@storybook/addon-links": "8.3.0 ",
    "@storybook/blocks": "8.3.0 ",
    "@storybook/builder-vite": "8.3.0 ",
    "@storybook/test": "8.3.0 ",
    "@storybook/vue3": "8.3.0 ",
    "@tailwindcss/typography": "0.5.15",
    "@testing-library/vue": "8.1.0",
    "@types/node": "22.7.5",
    "@vue/test-utils": "2.4.6",
    "dayjs-nuxt": "2.1.11",
    "happy-dom": "15.7.4",
    "nuxt": "3.13.2",
    "playwright-core": "1.48.0",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.8",
    "sass": "1.79.5",
    "schema-dts": "^1.1.2",
    "storybook": "8.3.0",
    "storybook-addon-root-attributes": "2.1.3",
    "vitest": "2.1.3",
    "vue": "3.5.12",
    "vue-i18n-extract": "2.0.7"
  },
  "packageManager": "pnpm@9.8.0+sha512.8e4c3550fb500e808dbc30bb0ce4dd1eb614e30b1c55245f211591ec2cdf9c611cabd34e1364b42f564bd54b3945ed0f49d61d1bbf2ec9bd74b866fcdc723276"
}