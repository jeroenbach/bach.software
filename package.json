{
  "name": "bach.software",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "pnpm run i18n-extract && nuxt dev",
    "generate": "pnpm run i18n-extract && nuxt generate && pnpm build-storybook",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "ssg": "nuxt generate && pnpm run serve",
    "serve": "pnpm dlx serve dist",
    "check-updates": "ncu -i --format group",
    "test": "TZ=Europe/Amsterdam vitest -w",
    "i18n-extract": "vue-i18n-extract report --vueFiles './**/*.?(js|vue)' --languageFiles './locales/*.?(json|yml|yaml|js)' --noEmptyTranslation '*' --add true --remove true",
    "storybook": "storybook dev --port 6006",
    "build-storybook": "storybook build -o ./.output/public/_storybook/",
    "ci:test": "TZ=Europe/Amsterdam vitest run",
    "azure": "swa start .output/public --api-location api/Bach.Software.Function",
    "function": "cd ./api/Bach.Software.Function && dotnet watch start"
  },
  "dependencies": {
    "@headlessui/vue": "1.7.23",
    "@heroicons/vue": "2.1.5",
    "@primevue/themes": "4.1.0",
    "@vueuse/core": "11.1.0",
    "animate.css": "4.1.1",
    "dayjs": "1.11.13",
    "luxon": "3.5.0",
    "nuxt-content-twoslash": "0.1.1",
    "pinia": "2.2.4",
    "primevue": "4.1.0",
    "sitemap": "8.0.0",
    "slugify": "1.6.6",
    "tailwindcss-primeui": "0.3.4",
    "typescript": "5.6.3"
  },
  "devDependencies": {
    "@nuxt/content": "2.13.2",
    "@nuxt/image": "1.8.1",
    "@nuxt/test-utils": "3.14.3",
    "@nuxtjs/color-mode": "3.5.1",
    "@nuxtjs/i18n": "8.5.5",
    "@nuxtjs/partytown": "1.5.0",
    "@nuxtjs/storybook": "8.3.0",
    "@nuxtjs/tailwindcss": "6.12.1",
    "@pinia/nuxt": "0.5.5",
    "@primevue/nuxt-module": "4.1.0",
    "@storybook-vue/nuxt": "8.3.0",
    "@storybook/addon-essentials": "8.3.0 ",
    "@storybook/addon-interactions": "8.3.0 ",
    "@storybook/addon-links": "8.3.0 ",
    "@storybook/blocks": "8.3.0 ",
    "@storybook/builder-vite": "8.3.0 ",
    "@storybook/test": "8.3.0 ",
    "@storybook/vue3": "8.3.0 ",
    "@tailwindcss/typography": "0.5.15",
    "@testing-library/vue": "8.1.0",
    "@types/node": "22.7.5",
    "@vue/test-utils": "2.4.6",
    "dayjs-nuxt": "2.1.11",
    "happy-dom": "15.7.4",
    "nuxt": "3.13.2",
    "playwright-core": "1.48.0",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.8",
    "sass": "1.79.5",
    "schema-dts": "^1.1.2",
